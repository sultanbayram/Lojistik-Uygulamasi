<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="16dp">
    <TextView
        android:id="@+id/tamamlananSoforEmail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Şoför Email: "
        android:textSize="16sp"/>
    <TextView
        android:id="@+id/tamamlananNereden"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nereden: "
        android:textSize="18sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/tamamlananNereye"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nereye: "
        android:textSize="18sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/tamamlananOlculer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Ölçüler: "
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/tamamlananFiyat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Fiyat: "
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/tamamlananTarih"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tarih:"
        android:textSize="16sp"/>



</LinearLayout>
